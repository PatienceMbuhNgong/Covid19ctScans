{
    "azureml_user":
    {
        "subscription_id": "<INSERT-YOUR-AZURE-SUBSCRIPTION-ID>",
        "resource_group": "<INSERT-YOUR-AZURE-RESOURCE-GROUP>",
        "workspace_name": "<CREATE-AZUREML-WORKSPACE-AND-INSERT-NAME>",
